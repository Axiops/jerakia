#!/usr/bin/env ruby

require 'jacaranda'
jac = Jacaranda.new


100.times do
req = Jacaranda::Request.new(
  :key => 'port',
  :namespace => [ 'apache' ],
  :policy => :craig,
  :lookup_type => :first,
  :metadata => { :environment => 'development' }
) 

answer = jac.lookup(req)
puts "---- FINAL OUTPUT ----"
p answer
end
