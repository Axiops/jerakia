#!/usr/bin/env ruby

require 'jacaranda'
jac = Jacaranda.new
req = Jacaranda::Request.new(
  :key => 'port',
  :namespace => [ 'apache' ],
  :policy => 'default',
  :lookup_type => :first,
  :metadata => { :environment => 'development' }
) 

p req

answer = jac.lookup(req)
puts "---- FINAL OUTPUT ----"
p answer
